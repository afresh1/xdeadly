<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>

	<head>
		<title><%= $title %></title>
	</head>

	<body bgcolor="#ffffff" text="#000000" link="#336666" vlink="#666666" alink="#996666">

		<%= include 'header' %>

		<table width="100%" cellpadding="0" cellspacing="0" border="0" bgcolor="#ffffff"><tbody>

			<tr>

				<td valign="top" width="100%">
					<%= include 'front_story' %>
					<p>
						<font color="#000000">
							<%= $navigation %>
						</font>
					</p>
				</td>

				<td valign="top" align="left">
					<table><tbody>
						<tr>
							<td valign="top">
								<table cellpadding="2" cellspacing="0" border="0" width="200"><tbody>
									<tr>
										<td bgcolor="#999999">
											<table bgcolor="#cccccc" cellpadding="2" cellspacing="0" border="0" width="198"><tbody>
												<tr bgcolor="#999999">
													<td valign="top">
														<font size="4" color="#336666">
															<b>Related Links</b>
														</font>
													</td>
												</tr>
												<tr>
													<td>
														<font size="-1">
															<%= $related %>
															<p>
														</font><p>
													</td>
												</tr>
											</tbody></table>
										</td>
									</tr>
								</tbody></table>
							</td>
						</tr>
					</tbody></table>
				</td>

			</tr>

		</tbody></table>

		<table width="100%" cellspacing="0" border="0"><tbody>
			<%= include comment_template, %{ $_ } for @{ $comments } %>
		</tbody></table>

		<%= include 'footer' %>

	</body>

</html>
